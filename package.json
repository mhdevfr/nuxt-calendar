{
  "name": "@nuxtjs/calendar",
  "version": "1.0.0",
  "description": "A customizable calendar component module for Nuxt 3",
  "author": "Milan Herbe",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "prepack": "nuxt-module-build build"
  },
  "dependencies": {
    "@nuxt/kit": "^3.8.0"
  },
  "peerDependencies": {
    "@vueuse/nuxt": "^10.0.0",
    "@nuxtjs/i18n": "^8.0.0",
    "lucide-vue-next": "^0.290.0"
  },
  "peerDependenciesMeta": {
    "@nuxtjs/i18n": {
      "optional": true
    },
    "lucide-vue-next": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.8.0",
    "@nuxt/test-utils": "^3.19.0",
    "@types/node": "^20.8.0",
    "@vueuse/nuxt": "^11.0.0",
    "changelogen": "^0.5.5",
    "eslint": "^8.52.0",
    "nuxt": "^3.8.0",
    "typescript": "latest",
    "vitest": "^3.2.0",
    "vue-tsc": "^2.0.0"
  },
  "keywords": [
    "nuxt",
    "nuxt3",
    "calendar",
    "vue",
    "component",
    "module",
    "date",
    "time"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/nuxt-module-calendar.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/nuxt-module-calendar/issues"
  },
  "homepage": "https://github.com/yourusername/nuxt-module-calendar#readme"
}